<?xml version="1.0" encoding="UTF-8"?>
<!-- Velos -->
<window id="2901">
	<defaultcontrol always="true">14</defaultcontrol>
	<allowoverlay>no</allowoverlay>
	<zorder>0</zorder>

	<controls>

		<control type="group">
			<left>0</left>
			<top>150</top>
			<animation effect="slide" start="0,-300" time="1000">WindowOpen</animation>
			<animation effect="slide" end="0,-300" time="500">WindowClose</animation>

			<control type="image">
				<left>0</left>
				<top>0</top>
				<width>1920</width>
				<height>1080</height>
				<texture>skindefault/colordiffuse/white60.png</texture>
				<animation effect="fade" start="100" end="0" time="500" condition="Window.IsVisible(SliderDialog) | Window.IsVisible(FileBrowser)">Conditional</animation>
				<visible>Window.IsVisible(osdvideosettings) | Window.IsVisible(osdaudiosettings)</visible>
			</control>
			
			<control type="image">
				<left>610</left>
				<top>0</top>
				<width>650</width>
				<height>85</height>
				<texture colordiffuse="$VAR[ColorDiffuseVideoOSDBackground]">skindefault/colordiffuse/white100solid.png</texture>
				<bordertexture colordiffuse="$VAR[ColorDiffuseVideoOSDBorder]">skindefault/colordiffuse/white100solid.png</bordertexture>
				<bordersize>2,0,2,2</bordersize>
				<animation effect="slide" start="0,-100" time="500">WindowOpen</animation>
				<animation effect="slide" end="0,-100" time="500">WindowClose</animation>
			</control>

			<control type="group">
				<visible>!Window.IsVisible(sliderdialog)</visible>
				
				<control type="label">
					<description>Current Control</description>
					<left>610</left>
					<top>30</top>
					<font>SmallCAP</font>
					<textcolor>$VAR[ColorDiffuseVideoOSDText]</textcolor>
					<width>650</width>
					<align>center</align>
					<label>$INFO[System.CurrentControl]</label>
				</control>

				<control type="grouplist" id="100">
					<left>460</left>
					<top>3</top>
					<width>500</width>
					<height>90</height>
					<itemgap>3</itemgap>
					<orientation>horizontal</orientation>
					<align>center</align>
					<onleft>6</onleft>
					<onright>12</onright>
					<onup>-</onup>
					<ondown>-</ondown>

					<control type="button" id="1">
						<description>Bookmarks</description>
						<width>50</width>
						<height>50</height>
						<label>298</label>
						<font>-</font>
						<texturefocus>other_textures/OSD/BookMarksFO.png</texturefocus>
						<texturenofocus>other_textures/OSD/BookMarksNF.png</texturenofocus>
						<onclick>ActivateWindow(125)</onclick>
					</control>

					<control type="button" id="2">
						<description>Audio Settings</description>
						<width>50</width>
						<height>50</height>
						<label>13396</label>
						<font>-</font>
						<texturefocus>other_textures/OSD/AudioFO.png</texturefocus>
						<texturenofocus>other_textures/OSD/AudioNF.png</texturenofocus>
						<onclick>ActivateWindow(124)</onclick>
					</control>

					<control type="button" id="3">
						<description>Video Settings</description>
						<width>50</width>
						<height>50</height>
						<label>13395</label>
						<align>left</align>
						<font>-</font>
						<texturefocus>other_textures/OSD/VideoFO.png</texturefocus>
						<texturenofocus>other_textures/OSD/VideoNF.png</texturenofocus>
						<onclick>ActivateWindow(123)</onclick>
					</control>

				</control>

				<control type="grouplist" id="101">
					<left>710</left>
					<top>3</top>
					<width>500</width>
					<height>120</height>
					<itemgap>3</itemgap>
					<orientation>horizontal</orientation>
					<align>center</align>
					<onleft>3</onleft>
					<onright>4</onright>
					<onup>-</onup>
					<ondown>-</ondown>

					<control type="button" id="12">
						<description>Skip Back</description>
						<width>50</width>
						<height>50</height>
						<label>210</label>
						<font>-</font>
						<texturefocus colordiffuse="$VAR[ColorDiffuseVideoOSDFocus]">skindefault/ui/osd/back.png</texturefocus>
						<texturenofocus colordiffuse="$VAR[ColorDiffuseVideoOSDNoFocus]">skindefault/ui/osd/back.png</texturenofocus>
						<onclick>PlayerControl(Previous)</onclick>
					</control>

					<control type="button" id="13">
						<description>Rewind</description>
						<width>50</width>
						<height>50</height>
						<label>31036</label>
						<font>-</font>
						<texturefocus colordiffuse="$VAR[ColorDiffuseVideoOSDFocus]">skindefault/ui/osd/rewind.png</texturefocus>
						<texturenofocus colordiffuse="$VAR[ColorDiffuseVideoOSDNoFocus]">skindefault/ui/osd/rewind.png</texturenofocus>
						<onclick>PlayerControl(Rewind)</onclick>
					</control>

					<control type="togglebutton" id="14">
						<description>Play/Pause</description>
						<width>50</width>
						<height>50</height>
						<label>31037</label>
						<altlabel>208</altlabel>
						<font>-</font>
						<texturefocus colordiffuse="$VAR[ColorDiffuseVideoOSDFocus]">skindefault/ui/osd/pause.png</texturefocus>
						<texturenofocus colordiffuse="$VAR[ColorDiffuseVideoOSDNoFocus]">skindefault/ui/osd/pause.png</texturenofocus>
						<usealttexture>Player.Paused | Player.Forwarding | Player.Rewinding</usealttexture>
						<alttexturefocus colordiffuse="$VAR[ColorDiffuseVideoOSDFocus]">skindefault/ui/osd/play.png</alttexturefocus>
						<alttexturenofocus colordiffuse="$VAR[ColorDiffuseVideoOSDNoFocus]">skindefault/ui/osd/play.png</alttexturenofocus>
						<onclick>PlayerControl(Play)</onclick>
					</control>

					<control type="button" id="15">
						<description>Stop</description>
						<width>50</width>
						<height>50</height>
						<label>31038</label>
						<font>-</font>
						<texturefocus colordiffuse="$VAR[ColorDiffuseVideoOSDFocus]">skindefault/ui/osd/stop.png</texturefocus>
						<texturenofocus colordiffuse="$VAR[ColorDiffuseVideoOSDNoFocus]">skindefault/ui/osd/stop.png</texturenofocus>
						<onclick>PlayerControl(Stop)</onclick>
					</control>

					<control type="button" id="16">
						<description>Fast Forward</description>
						<width>50</width>
						<height>50</height>
						<label>31039</label>
						<font>-</font>
						<texturefocus colordiffuse="$VAR[ColorDiffuseVideoOSDFocus]">skindefault/ui/osd/fastforward.png</texturefocus>
						<texturenofocus colordiffuse="$VAR[ColorDiffuseVideoOSDNoFocus]">skindefault/ui/osd/fastforward.png</texturenofocus>
						<onclick>PlayerControl(Forward)</onclick>
					</control>

					<control type="button" id="17">
						<description>Next</description>
						<width>50</width>
						<height>50</height>
						<label>209</label>
						<font>-</font>
						<texturefocus colordiffuse="$VAR[ColorDiffuseVideoOSDFocus]">skindefault/ui/osd/forward.png</texturefocus>
						<texturenofocus colordiffuse="$VAR[ColorDiffuseVideoOSDNoFocus]">skindefault/ui/osd/forward.png</texturenofocus>
						<onclick>PlayerControl(Next)</onclick>
					</control>

				</control>

				<control type="grouplist" id="102">
					<left>935</left>
					<top>3</top>
					<width>500</width>
					<height>90</height>
					<itemgap>3</itemgap>
					<orientation>horizontal</orientation>
					<align>center</align>
					<onleft>17</onleft>
					<onright>1</onright>
					<onup>-</onup>
					<ondown>-</ondown>

					<control type="button" id="4">
						<description>Subtitles</description>
						<width>50</width>
						<height>50</height>
						<label>287</label>
						<font>-</font>
						<texturefocus>other_textures/OSD/SubtitlesFO.png</texturefocus>
						<texturenofocus>other_textures/OSD/SubtitlesNF.png</texturenofocus>
						<onclick>Close</onclick>
						<onclick>XBMC.RunScript(script.xbmc.subtitles)</onclick>
						<visible>IsEmpty(Skin.String(SubtitlesPath))</visible>
					</control>

					<control type="button" id="5">
						<description>Subtitles</description>
						<width>50</width>
						<height>50</height>
						<label>287</label>
						<font>-</font>
						<texturefocus>other_textures/OSD/SubtitlesFO.png</texturefocus>
						<texturenofocus>other_textures/OSD/SubtitlesNF.png</texturenofocus>
						<onclick>Close</onclick>
						<onclick>XBMC.RunScript($INFO[Skin.String(SubtitlesPath)])</onclick>
						<visible>!IsEmpty(Skin.String(SubtitlesPath))</visible>
					</control>

					<control type="button" id="6">
						<description>DVD menu</description>
						<width>50</width>
						<height>50</height>
						<label>31041</label>
						<font>-</font>
						<texturefocus>other_textures/OSD/DvdFO.png</texturefocus>
						<texturenofocus>other_textures/OSD/DvdNF.png</texturenofocus>
						<onclick>PlayerControl(ShowVideoMenu)</onclick>
						<enable>VideoPlayer.HasMenu</enable>
					</control>

				</control>

			</control>

		</control>

	</controls>

</window>
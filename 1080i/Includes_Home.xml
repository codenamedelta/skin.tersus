<?xml version="1.0" encoding="utf-8"?>
<includes>
	
	<!-- Home Icons -->
	<include name="HomeScreenIcons">
		
		<control type="image">
			<left>96</left>
			<top>140</top>
			<width>800</width>
			<height>800</height>
			<texture colordiffuse="$VAR[ColorDiffuseHomeIcons]">$VAR[HomeIcons]</texture>
			<fadetime>200</fadetime>
			<visible>!Skin.HasSetting(HideIcons)</visible>
			<animation effect="fade" start="0" end="100" delay="600" time="200">WindowOpen</animation>
		</control>
	
	</include>	

</includes>